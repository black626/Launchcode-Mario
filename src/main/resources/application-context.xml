<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <!-- grab reference for 'greedy1' object using usCC and euCC as parameters-->
  <bean id="usCC" class="USCoinCalculator"> </bean>

  <bean id="euCC" class="EUCoinCalculator"> </bean>

  <bean id="greedy1" class="Greedy">
  <constructor-arg name="usCC" ref="usCC"/>
    <constructor-arg name="euCC" ref="euCC"/>
  </bean>


    
</beans>
